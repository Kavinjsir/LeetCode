/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) { val = x; }
 * }
 */
class Solution {
    public ListNode rotateRight(ListNode head, int k) {
        if(head == null){
            return head;
        }
        
        int len = 1;
        ListNode p = head;
        while(p.next != null){
            p = p.next;
            len++;
        }
        k %= len;
        ListNode end = p;
        end.next = head;
        len -= k;
        int i = 1;
        p = head;
        while(i < len){
            p = p.next;
            i++;
        }
        ListNode q = p.next;
        p.next = null;
        
        
        return q;
    }
}
